#  pod 'FirebaseUI/Facebook'
  pod 'FirebaseUI/Phone'
#  pod 'MaterialComponents'
  pod 'Crashlytics'
  pod 'Fabric'
  pod 'SwiftFormat'
  pod 'SnapKit'
  pod 'CodableFirebase'
  pod 'GoogleDataTransport'

  target 'AnnotateARTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AnnotateARUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

